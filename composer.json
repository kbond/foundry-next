{
    "name": "zenstruck/foundry",
    "description": "A model factory library for creating expressive, auto-completable, on-demand dev/test fixtures with Symfony and Doctrine.",
    "homepage": "https://github.com/zenstruck/foundry",
    "type": "library",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/persistence": "^2.0|^3.0",
        "fakerphp/faker": "^1.23",
        "symfony/framework-bundle": "^6.3",
        "symfony/property-access": "^6.3",
        "symfony/var-exporter": "^6.3",
        "zenstruck/assert": "^1.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.0",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/var-dumper": "^6.3"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Foundry\\": ["src/"] },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Foundry\\Tests\\": ["tests/"] }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
